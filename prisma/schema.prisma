datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// generator
generator client {
  provider             = "prisma-client-py"
  recursive_type_depth = -1
}

// Sample Schema for tesing, will allow app to run with Prisma after first install
model User {
  id       Int    @id @default(autoincrement())
  username String @unique
}
